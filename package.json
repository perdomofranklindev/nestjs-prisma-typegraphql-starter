    "@apollo/gateway": "^2.4.8",
    "@apollo/server": "^4.7.5",
    "@apollo/subgraph": "^2.4.8",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.5",
    "@nestjs/core": "^10.0.5",
    "@nestjs/graphql": "^12.0.7",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.5",
    "@types/express": "^4.17.17",
