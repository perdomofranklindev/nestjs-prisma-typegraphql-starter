    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.1.2",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^13.2.3",
